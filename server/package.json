{
  "name": "autha-proposal-generator-server",
  "version": "1.0.0",
  "description": "Backend para geração de propostas orçamentárias Autha",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "clean": "rimraf dist",
    "copy-templates": "copyfiles -u 1 \"src/templates/**/*\" dist",
    "build": "npm run clean && tsc && npm run copy-templates",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "autha",
    "proposal",
    "pdf",
    "puppeteer"
  ],
  "author": "Autha Engenharia",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.3",
    "handlebars": "^4.7.8",
    "puppeteer": "^24.7.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.5.1",
    "@types/node": "^20.10.5",
    "@types/puppeteer": "^7.0.4",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
